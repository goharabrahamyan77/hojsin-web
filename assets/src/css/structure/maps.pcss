.map-box {
    padding: 25px;
    background: #f8f8f8;
    border-radius: 5px;
    box-shadow: 1px 2px 6px rgba(0,0,0,.25);
    min-height: 265px;
    display:flex;
    align-items: center;
    justify-content: center;

    img {
        margin: 0;
        max-height: 215px;
    }
}

.maps {
    &__item {
        margin-bottom: 30px;

        @media (--md-viewport) {
            margin-bottom: 0;
        }
    }
}
