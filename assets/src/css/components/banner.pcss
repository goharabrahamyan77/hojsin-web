.section-banner {
    .jarallax {
        &-table{
            display: table;
            width: 100%;
            height: 340px;

            @media (--md-viewport) {
                height: 440px;
            }

            @media (--lg-viewport) {
                height: 640px;
            }

            &-cell {
                display: table-cell;
                text-align: center;
                vertical-align: middle
            }
        }
    }
}
